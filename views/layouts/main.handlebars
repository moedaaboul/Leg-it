<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>leg-it</title>
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css'
      integrity='sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    />
    <link
      rel='stylesheet'
      href='https://cdn.jsdelivr.net/npm/bulma@0.9.3/css/bulma.min.css'
    />
    <link rel='stylesheet' href='/css/styles.css' />
    {{#if login}}
      <link rel='stylesheet' href='/css/login.css' />
    {{/if}}
    {{#if register}}
      <link rel='stylesheet' href='/css/register.css' />
    {{/if}}
    {{#if singlePost}}
      <link rel='stylesheet' href='/css/single-post.css' />
    {{/if}}
  </head>
  <body>
    {{{body}}}
    <script
      src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js'
      integrity='sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    ></script>
    <script src='/js/auth.js'></script>

    <div class='footerWrapper'>
      <footer id='footer' class='columns'>

        <div class='column is-one-quarter is-desktop'></div>

        <div class='column is-desktop'>
          <div class='m-1 title is-5 mg-medium has-text-centered'>
            <p> Source </p>
            <br />
            <a href='https://github.com/heranYang93/Leg-it'>
              <i class='fab fa-github'></i>
            </a>
          </div>
        </div>

        <div class='column is-desktop'>
          <div class='m-1 title is-5 mg-medium has-text-centered'>
            <p> Report an Error </p>
            <br />
            <a href='https://github.com/heranYang93/Leg-it/issues/new'>
              <i class='fas fa-exclamation-triangle'></i>
            </a>
          </div>
        </div>

        <div class='column is-desktop is-one-quarter'></div>

      </footer>
    </div>
  </body>
</html>